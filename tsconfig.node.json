{
	"compilerOptions": {
		"target": "ES2022",
		"lib": ["ES2023"],
		"module": "ESNext",
		"skipLibCheck": true,
		"moduleResolution": "bundler",
		"isolatedModules": true,
		"moduleDetection": "force",
		"composite": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"types": ["node", "react/canary"]
	},
	"include": ["vite.config.ts", "vite-plugin-mdx-metadata.ts", "scripts"]
}
